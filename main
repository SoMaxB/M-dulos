#importando el modulo de funciones(suma, resta, multi, divi)

import Funciones


def main():
    cuenta0 = Funciones.suma(1, 3)
    cuenta1 = Funciones.resta(2, 1)
    cuenta2 = Funciones.multi(2, 5)
    cuenta3 = Funciones.divi(8, 4)
    print("Estas son mis funciones ", cuenta0, cuenta1, cuenta2, cuenta3)




if __name__ == '__main__':
        main()
